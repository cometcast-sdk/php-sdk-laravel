{
    "name": "cometcast/php-sdk-laravel",
    "description": "",
    "keywords": [
        "cometcast",
        "php-sdk-laravel"
    ],
    "homepage": "https://github.com/cometcast/php-sdk-laravel",
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "Chou, Yu-Hsien",
            "email": "slps970093@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "cometcast/openapi-php-sdk": "v20251117-beta",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cometcast\\OpenApi\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cometcast\\OpenApi\\Laravel\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cometcast\\PhpSdkLaravel\\PhpSdkLaravelServiceProvider"
            ],
            "aliases": {
                "PhpSdkLaravel": "Cometcast\\PhpSdkLaravel\\PhpSdkLaravelFacade"
            }
        }
    }
}
